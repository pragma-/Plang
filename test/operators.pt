test_eq(1 + 4 * 3 + 2 * 4, 21, "arithmetic 1")
test_eq((1 + 4) * (3 + 2) * 4, 100, "arithmetic 2")
test_eq((1 + 4) * (((5 + 5) * 4) / 10) / 2, 10, "arithmetic 3")

test_eq(1e2 + 1e3, 1100.0, "exponent literals")

test_eq(1e-4, 0.0001, "negative exponent literal")

test_eq("hi" && 42, 42, "logical and")
test_eq("" && 42, "", "logical and")
test_eq("" || 42, 42, "logical or")
test_eq(1 == 1, true, "equality")
